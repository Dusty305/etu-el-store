{
  "name": "etu-el-store",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "cd client && npm run dev",
    "dev:server": "cd server && npm run dev",
    "ci": "npm ci && cd client && npm ci && cd ../server && npm ci",
    "build": "cd client && npm run build",
    "start": "cd server && npm start",
    "load-db": "cd server && npm run load-db"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "dependencies": {
    "asdasdas": "^1.1.0"
  }
}
